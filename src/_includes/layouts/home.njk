  <div class="content">
    <section class="intro">
      <div class="info">
        <div class="short">
          {{ short | safe }}
        </div>
        <button onclick="toggleCaption(this)" class="button" aria-label="Expand/collapse caption">...</button>
        <div class="caption">
          {{ content | safe }}
        </div>
      </div>
    </section>
    {% for artist in collections.artist %}
    <section class="artist {{ artist.data.artist }}" id="{{ artist.data.artist }}" data-gradient="{{ artist.data.artist }}-gradient">
      <div class="info">
        <h1>{{ artist.data.title }}</h1>
        {% if artist.data.instagram|length %}
        <button onclick="toggleCaption(this)" class="button" aria-label="Expand/collapse caption">...</button>
        <div class="caption" >
          {% if artist.data.description|length %}
          {{ artist.data.description | safe }}
          <br><br>
          {% endif %}
          <a href="https://instagram.com/{{ artist.data.instagram }}" target="_blank">@{{ artist.data.instagram }}</a>
        </div>
        {% endif %}
      </div>
      <ul>
      {% for post in collections.post %}
        {% if artist.data.artist in post.outputPath %}
        <li>
          <div class="carousel">
            {% for filename in post.data.media %}
            <figure>
              {% if '.mp4' in filename %}
              <video
                class="player lozad"
                controls playsinline preload="metadata"
                data-placeholder-background="rgba(255,255,255,.2)"
                aria-label="{{ post.data.alt }}"
                {% if post.data.longdesc[loop.index0]|length %}
                aria-describedby="{{ post.data.longdesc[loop.index0] }}"
                {% endif %}
                >
                <source data-src="{{ post.url }}{{ filename | url }}#t=0.1" type="video/mp4">
              </video>
              {% else %}
              <img
                data-flickity-lazyload="{{ post.url }}{{ filename | url }}"
                alt="{{ post.data.alt[loop.index0] }}"
                {% if post.data.longdesc[loop.index0]|length %}
                longdesc="{{ post.data.longdesc[loop.index0] }}"
                {% endif %}
                >
              {% endif %}
            </figure>
            {% endfor %}
          </div>
          {% if post.data.content|length %}
          <button onclick="toggleCaption(this)" class="button" aria-label="Expand/collapse caption">...</button>
          <div class="caption">{{ post.data.content | safe }}</div>
          {% endif %}
        </li>
        {% endif %}
      {% endfor %}
      </ul>
    </section>
    {% endfor %}
  </div>
</main>
<div class="background">
  {% for artist in collections.artist %}
  <div class="background-gradient {{ artist.data.artist }}-gradient"></div>
  {% endfor %}
</div>
